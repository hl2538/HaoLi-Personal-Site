<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.haoli.dao.UserDao">

    <select id = "selectUserByLoginName" parameterType="java.util.Map" resultType = "com.haoli.domain.User">
    	select *
    	from
    		user
    	where
    		loginName = #{loginName}
    </select>
    
</mapper>